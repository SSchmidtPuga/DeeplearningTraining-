","  if len (y_pred[0]) >1:\n","    print(\"doing multiclass clasification\")\n","    y_pred = np.argmax(y_pred, axis = 1).reshape(xx.shape)\n","\n","  else:\n","    print(\"Doing binary clasification\")\n","    y_pred = np.round(y_pred).reshape(xx.shape)\n","\n","  plt.contourf(xx,yy,y_pred, cmap=plt.cm.RdYlBu, alpha=0.7)\n","  \n","  plt.scatter(X[:,0], X[:,1], c=y,s=40,cmap=plt.cm.RdYlBu)\n","  plt.xlim(xx.min(), xx.max())\n","  plt.ylim(yy.min(), yy.max())\n","\n","\n","plot_decision_boundary(model_1, X=X, y=y)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":282},"id":"eSMa7sjZastD","executionInfo":{"status":"ok","timestamp":1650065161957,"user_tz":240,"elapsed":1026,"user":{"displayName":"Sebastian Schmidt","userId":"09571167048820773400"}},"outputId":"607136f4-8df0-44e6-de55-2724c6686049"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Doing binary clasification\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":["pd.DataFrame(history.history).plot()\n","plt.ylabel(\"lose\")\n","plt.xlabel(\"expochs\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":296},"id":"wz_N-tO4a_s9","executionInfo":{"status":"ok","timestamp":1650063560798,"user_tz":240,"elapsed":262,"user":{"displayName":"Sebastian Schmidt","userId":"09571167048820773400"}},"outputId":"b2234d71-211b-42e5-aff9-d3232d2f43a2"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["Text(0.5, 0, 'expochs')"]},"metadata":{},"execution_count":45},{"output_type":"display_data","data":{"text/plain":["<Figure size 432x288 with 1 Axes>"],"image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":[""],"metadata":{"id":"Yh0iOLiobaBY"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["Checking learning optimal rate\n"],"met